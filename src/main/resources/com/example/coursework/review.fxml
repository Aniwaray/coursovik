<AnchorPane prefHeight="358.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.Review">
   <children>
      <Label layoutX="59.0" layoutY="26.0" text="Ваши отзывы помогают нам становиться лучше!">
         <font>
            <Font name="Comic Sans MS Bold" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="textRating" layoutX="204.0" layoutY="125.0" prefHeight="26.0" prefWidth="117.0" promptText="Оценка (от 0 до 5)" style="-fx-border-color: black;" />
