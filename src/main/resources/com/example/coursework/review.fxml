      <TextArea fx:id="textComment" layoutX="164.0" layoutY="178.0" prefHeight="88.0" prefWidth="206.0" promptText="Комментарий" style="-fx-border-color: black;" />
      <Button fx:id="buttonSend" layoutX="223.0" layoutY="297.0" mnemonicParsing="false" style="-fx-background-color: white; -fx-border-color: black;" text="Отправить">
